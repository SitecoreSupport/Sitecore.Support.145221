<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery">
    <hooks>
      <hook type="Sitecore.Analytics.BackgroundServiceLoader, Sitecore.Analytics">
        <patch:attribute name="type">Sitecore.Support.Analytics.BackgroundServiceLoader, Sitecore.Support.145221</patch:attribute>
      </hook>
    </hooks>

    <submitQueue>
      <backgroundService type="Sitecore.Analytics.SubmitQueueService, Sitecore.Analytics">
        <patch:attribute name="type">Sitecore.Support.Analytics.SubmitQueueService, Sitecore.Support.145221</patch:attribute>
      </backgroundService>
	  <existingRecordQueue type="Sitecore.Analytics.Data.DataAccess.SubmitQueue.FileSubmitQueue, Sitecore.Analytics" singleInstance="true">
		<param desc="folderPath">$(dataFolder)/Submit Queue/Existing</param>
	  </existingRecordQueue>
    </submitQueue>
	
  </sitecore>
</configuration>